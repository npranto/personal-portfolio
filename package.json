{
	"name": "personal-portfolio",
	"version": "5.0.0",
	"scripts": {
		"dev": "npm run fetch-content && next dev",
		"build": "npm run fetch-content && next build",
		"start": "npm run fetch-content && next start",
		"lint": "next lint",
		"prepare": "husky",
		"format": "npx prettier --write .",
		"fetch-content": "npm run fetch-blog-posts && npm run fetch-video-posts",
		"fetch-blog-posts": "node --no-warnings ./src/scripts/fetch-blog-posts.mjs",
		"fetch-video-posts": "node --no-warnings ./src/scripts/fetch-video-posts.mjs"
	},
	"dependencies": {
		"@next/third-parties": "^15.1.4",
		"@sendgrid/mail": "^8.1.3",
		"chalk": "^5.3.0",
		"dotenv": "^16.4.5",
		"next": "^15.1.4",
		"react": "^18",
		"react-dom": "^18",
		"sharp": "^0.33.5",
		"zod": "^3.23.8",
		"zod-validation-error": "^3.3.1"
	},
	"devDependencies": {
		"contentful": "^11.0.2",
		"eslint": "^8",
		"eslint-config-next": "14.2.6",
		"fs": "^0.0.1-security",
		"husky": "^9.1.5",
		"postcss": "^8",
		"prettier": "^3.3.3",
		"puppeteer": "^23.11.1",
		"tailwindcss": "^3.4.1"
	}
}
