{
  "name": "personal-portfolio",
  "version": "4.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "npx rimraf public node_modules && npm install",
    "tailwind:build": "npx tailwindcss -c ./public/vendor/tailwind/tailwind.config.js -i ./public/vendor/tailwind/input.css -o ./public/vendor/tailwind/output.css",
    "tailwind:watch": "npx tailwindcss -c ./public/vendor/tailwind/tailwind.config.js -i ./public/vendor/tailwind/input.css -o ./public/vendor/tailwind/output.css --watch",
    "lint": "npx eslint ./",
    "format": "npx prettier --write \"**/*.{html,css,js,json}\"",
    "dev": "npx concurrently \"npm run tailwind:watch\" \"eleventy --serve\"",
    "build": "npx rimraf public && eleventy && npm run tailwind:build",
    "preview": "npx serve public -l 9000",
    "cypress:open": "npx cypress open --config-file cypress.dev.json",
    "cypress:run:dev": "npx cypress --config-file cypress.dev.json run",
    "cypress:run:prod": "npx cypress --config-file cypress.json run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npranto/personal-portfolio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/npranto/personal-portfolio/issues"
  },
  "homepage": "https://github.com/npranto/personal-portfolio#readme",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "contentful": "^9.1.12",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "chalk": "^4.1.2",
    "concurrently": "^6.5.1",
    "cypress": "^9.5.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "node-fetch": "^3.2.1",
    "prettier": "^2.5.1",
    "puppeteer": "^13.4.1",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "sharp": "^0.29.3",
    "tailwindcss": "^3.0.23"
  }
}
